<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<RootNamespace>ArsVeneficiSpellPartModTemplate</RootNamespace>
		<ImplicitUsings>enable</ImplicitUsings>
		<EnableHarmony>true</EnableHarmony>
		<GamePath>$(StardewValleyGamePath)</GamePath>
		<BaseOutputPath></BaseOutputPath>
	</PropertyGroup>

	<ItemGroup>
		<Compile Remove="obj\**" />
		<EmbeddedResource Remove="obj\**" />
		<None Remove="obj\**" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Pathoschild.Stardew.ModBuildConfig" Version="4.3.2" />
		<Reference Include="ArsVenefici" HintPath="$(StardewValleyGamePath)\Mods\ArsVenefici\ArsVenefici\ArsVenefici.dll" Private="False" />
	</ItemGroup>

	<ItemGroup>
		<ContentPacks Include="../[CP] Ars Venefici Spell Part Template" Version="$(Version)" />
	</ItemGroup>

</Project>
